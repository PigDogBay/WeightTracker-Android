    <string name="report_template">
        <![CDATA[

<html>
<head>
	<style>
		table {font-family: arial, sans-serif; border-collapse: collapse; width: 50%;}
		th {border: 1px solid #dddddd; text-align: left; padding: 6px;}
		td {border: 1px solid #dddddd; text-align: left; padding: 6px; width: 50%}
	</style>
</head>
<body>
<h1>Weight Tracker Report</h1>
$Date<br/><br/>
<table>
	<tr><th colspan="2">Indice Massa Corporea (BMI)</th></tr>
	<tr>
		<td>Ultimo</td><td>$LatestBMI</td>
	</tr>
	<tr>
		<td>Media BMI</td><td>$AverageBMI</td>
	</tr>
	<tr>
		<td>Obiettivo</td><td>$GoalBMI</td>
	</tr>
	<tr>
		<td>Peso ideale</td><td>$IdealRange</td>
	</tr>
</table>
<br/>
<table>
	<tr><th colspan="2">Ultimi 7 giorni</th></tr>
	<tr>
		<td>$TrendWeekTitle</td><td>$TrendWeekValue</td>
	</tr>
	<tr>
		<td>Data dell\'obiettivo</td><td>$TrendWeekGoalDate</td>
	</tr>
	<tr><th colspan="2">Ultimi 30 giorni</th></tr>
	<tr>
		<td>$TrendMonthTitle</td><td>$TrendMonthValue</td>
	</tr>
	<tr>
		<td>Goal Date</td><td>$TrendMonthGoalDate</td>
	</tr>
	<tr><th colspan="2">All Time</th></tr>
	<tr>
		<td>$TrendAllTitle</td><td>$TrendAllValue</td>
	</tr>
	<tr>
		<td>Data dell\'obiettivo</td><td>$TrendAllGoalDate</td>
	</tr>
</table>
<br/>
<table>
	<tr><th colspan="2">Peso</th></tr>
	<tr>
		<td>Media Peso</td><td>$AverageWeight</td>
	</tr>
	<tr>
		<td>Peso min</td><td>$MinWeight</td>
	</tr>
	<tr>
		<td>Peso max</td><td>$MaxWeight</td>
	</tr>
	<tr>
		<td>Max - Min</td><td>$MaxMinusMinWeight</td>
	</tr>
	<tr>
		<td>Peso iniziale</td><td>$FirstWeight</td>
	</tr>
	<tr>
		<td>Ultimo peso</td><td>$LastWeight</td>
	</tr>
	<tr>
		<td>Primo - Ultimo</td><td>$FirstMinusLastWeight</td>
	</tr>
</table>
<br/>
<table>
	<tr><th colspan="2">Statistica</th></tr>
	<tr>
		<td>Numero di pesate</td><td>$Count</td>
	</tr>
	<tr>
		<td>Data d\'inizio</td><td>$FirstDate</td>
	</tr>
	<tr>
		<td>Data Ultima pesata</td><td>$LastDate</td>
	</tr>
	<tr>
		<td>Giorni trascorsi</td><td>$TimeSpent</td>
	</tr>
</table>
</body>
</html>
]]>
	</string>
